<Page loaded="onPageLoaded" xmlns:chart="ui-for-nativescript/chart" xmlns="http://www.nativescript.org/tns.xsd">
    <chart:CartesianChart id="cartesianChart">
    <chart:CartesianChart.series>
          <chart:CandlestickSeries
            categoryProperty="Date"
            openPropertyName="Open"
            highPropertyName="High"
            lowPropertyName="Low"
            closePropertyName="Close"
            items="{{ candleStickSourceItems }}">
              <chart:CandlestickSeries.horizontalAxis>
                <chart:CategoricalAxis/>
              </chart:CandlestickSeries.horizontalAxis>
              <chart:CandlestickSeries.verticalAxis>
                <chart:LinearAxis/>
              </chart:CandlestickSeries.verticalAxis>
            </chart:CandlestickSeries>
        </chart:CartesianChart.series>
    </chart:CartesianChart>
</Page>
