<Page loaded="onPageLoaded" xmlns:chart="nativescript-telerik-ui/chart" xmlns="http://www.nativescript.org/tns.xsd">
    <chart:RadCartesianChart id="cartesianChart">
    <chart:RadCartesianChart.series>
          <chart:OhlcSeries
            categoryProperty="Date"
            openPropertyName="Open"
            highPropertyName="High"
            lowPropertyName="Low"
            closePropertyName="Close"
            items="{{ ohlcSourceItems }}">
              <chart:OhlcSeries.horizontalAxis>
                <chart:CategoricalAxis/>
              </chart:OhlcSeries.horizontalAxis>
              <chart:OhlcSeries.verticalAxis>
                <chart:LinearAxis/>
              </chart:OhlcSeries.verticalAxis>
            </chart:OhlcSeries>
        </chart:RadCartesianChart.series>
    </chart:RadCartesianChart>
</Page>
