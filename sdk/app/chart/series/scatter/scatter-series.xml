<Page loaded="onPageLoaded" xmlns:chart="nativescript-telerik-ui/chart" xmlns="http://schemas.nativescript.org/tns.xsd">
    <chart:RadCartesianChart id="cartesianChart">
        <chart:RadCartesianChart.series>
            <chart:ScatterSeries items="{{ scatterSource }}" xProperty="Age" yProperty="Salary">
                <chart:ScatterSeries.horizontalAxis>
                    <chart:LinearAxis/>
                </chart:ScatterSeries.horizontalAxis>
                <chart:ScatterSeries.verticalAxis>
                    <chart:LinearAxis/>
                </chart:ScatterSeries.verticalAxis>
            </chart:ScatterSeries>
        </chart:RadCartesianChart.series>
    </chart:RadCartesianChart>
</Page>
