        <chart:RadCartesianChart.series>
            <chart:AreaSeries items="{{ categoricalSource }}" stackMode="Stack" categoryProperty="Country" valueProperty="Amount">

            </chart:AreaSeries>
            <chart:AreaSeries items="{{ categoricalSource }}" stackMode="Stack" categoryProperty="Country" valueProperty="Amount">

            </chart:AreaSeries>
            <chart:AreaSeries items="{{ categoricalSource }}" stackMode="Stack" categoryProperty="Country" valueProperty="Amount">

