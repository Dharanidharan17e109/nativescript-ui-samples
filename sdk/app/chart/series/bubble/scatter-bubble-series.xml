<Page loaded="onPageLoaded" xmlns:chart="nativescript-telerik-ui/chart" xmlns="http://schemas.nativescript.org/tns.xsd">
    <chart:RadCartesianChart id="pieChart">
        <chart:RadCartesianChart.series>
            <chart:ScatterBubbleSeries  items="{{ scatterBubbleSource }}" bubbleScale="5" xProperty="xValue" yProperty="yValue" bubbleSizeProperty="Impact" >
                <chart:ScatterBubbleSeries.horizontalAxis>
                    <chart:LinearAxis minimum="20" maximum="100"/>
                </chart:ScatterBubbleSeries.horizontalAxis>
                <chart:ScatterBubbleSeries.verticalAxis>
                    <chart:LinearAxis minimum="20" maximum="100"/>
                </chart:ScatterBubbleSeries.verticalAxis>
            </chart:ScatterBubbleSeries>
        </chart:RadCartesianChart.series>
    </chart:RadCartesianChart>
</Page>
