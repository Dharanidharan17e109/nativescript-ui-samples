<navigation:ExamplePage loaded="onPageLoaded"  xmlns:navigation="navigation/example-page" xmlns:lv="nativescript-telerik-ui-pro/autocomplete" xmlns="http://www.nativescript.org/tns.xsd">
    <StackLayout>
<!-- >> autocomplete-getting-started -->
    <lv:RadAutoCompleteTextView items="{{ dataItems }}"/>
<!-- << autocomplete-getting-started -->

<!-- >> autocomplete-completion-mode -->
    <lv:RadAutoCompleteTextView completionMode="Contains"/>
<!-- << autocomplete-completion-mode -->

<!-- >> autocomplete-suggest-mode -->
<lv:RadAutoCompleteTextView suggestMode="Append"/>
<!-- << autocomplete-suggest-mode -->

<!-- >> autocomplete-layout-mode -->
<lv:RadAutoCompleteTextView layoutMode="Wrap"/>
<!-- << autocomplete-layout-mode -->

<!-- >> autocomplete-display-mode -->
<lv:RadAutoCompleteTextView displayMode="Tokens"/>
<!-- << autocomplete-display-mode -->

    </StackLayout>
</navigation:ExamplePage>
