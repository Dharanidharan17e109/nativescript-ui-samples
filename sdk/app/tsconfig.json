{
    "version": "1.8.7",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": false
    },
    "filesGlob": [
        "./**/*.ts",
        "./../node_modules/tns-core-modules/**/*.ts",
        "./../node_modules/nativescript-telerik-ui-pro/**/*.ts"
    ],
    "exclude": [],
    "include": [
        "../node_modules"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
